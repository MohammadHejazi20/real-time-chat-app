#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mohammadhejazi/Projects/private/real-time-chat-app/node_modules/.pnpm/rolldown@1.0.0-beta.34/node_modules/rolldown/bin/node_modules:/Users/mohammadhejazi/Projects/private/real-time-chat-app/node_modules/.pnpm/rolldown@1.0.0-beta.34/node_modules/rolldown/node_modules:/Users/mohammadhejazi/Projects/private/real-time-chat-app/node_modules/.pnpm/rolldown@1.0.0-beta.34/node_modules:/Users/mohammadhejazi/Projects/private/real-time-chat-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mohammadhejazi/Projects/private/real-time-chat-app/node_modules/.pnpm/rolldown@1.0.0-beta.34/node_modules/rolldown/bin/node_modules:/Users/mohammadhejazi/Projects/private/real-time-chat-app/node_modules/.pnpm/rolldown@1.0.0-beta.34/node_modules/rolldown/node_modules:/Users/mohammadhejazi/Projects/private/real-time-chat-app/node_modules/.pnpm/rolldown@1.0.0-beta.34/node_modules:/Users/mohammadhejazi/Projects/private/real-time-chat-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/rolldown@1.0.0-beta.34/node_modules/rolldown/bin/cli.mjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/rolldown@1.0.0-beta.34/node_modules/rolldown/bin/cli.mjs" "$@"
fi
